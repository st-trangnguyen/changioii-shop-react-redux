{
  "name": "typescript-react-redux-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "A frontend boilerplate with React, Redux and Typescript",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --env.NODE_ENV=development --debug --devtool cheap-module-source-map --output-pathinfo --history-api-fallback --hot --inline --progress --colors --port 3001",
    "build.prod": "npx webpack --env.NODE_ENV=production",
    "build": "npx webpack",
    "lint": "eslint -f ./my-awesome-formatter.js --output-file ./eslint.log.json --ext .tsx src",
    "build:main": "npx webpack --config webpack.config.main.js",
    "build:electron": "npm run build:main && npm run build",
    "electron:package": "npm run build:electron && electron-builder -mw",
    "electron:package-mac": "npm run build:electron && electron-builder --mac"
  },
  "build": {
    "extends": null,
    "files": [
      "dist/",
      "package.json"
    ],
    "productName": "ChangioiiShop",
    "appId": "com.electron.changioii",
    "directories": {
      "output": "release",
      "buildResources": "dist"
    },
    "mac": {
      "target": "dmg",
      "category": "public.app-category.utilities",
      "icon": "src/assets/icons/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "src/assets/icons/favicon-1024x1024.ico"
    }
  },
  "author": "Trang Nguyen <trang.nguyen@supremetech.vn>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@types/classnames": "2.2.3",
    "@types/history": "4.6.2",
    "@types/node": "8.0.53",
    "@types/react": "^16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-redux": "^7.1.8",
    "@types/react-router-dom": "5.1.3",
    "@types/redux-actions": "2.2.2",
    "@types/webpack": "3.8.1",
    "@types/webpack-env": "1.13.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "awesome-typescript-loader": "^3.4.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "electron": "^12.0.5",
    "electron-builder": "^22.10.5",
    "electron-is-dev": "^2.0.0",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.7.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.14.1",
    "postcss": "^6.0.14",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.2.1",
    "react-hot-loader": "^3.1.3",
    "react-router-dom": "^5.1.2",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^6.0.6",
    "script-loader": "^0.7.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.19.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "url-loader": "^0.6.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "element-react": "^1.4.34",
    "element-theme-default": "^1.4.13",
    "i18next": "^19.4.4",
    "i18next-http-backend": "^1.0.10",
    "react": "^16.13.0",
    "react-animated-css": "^1.2.1",
    "react-dom": "^16.13.0",
    "react-i18next": "^11.4.0",
    "react-redux": "^7.2.0",
    "react-swipeable": "^5.5.1",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "reset-css": "^5.0.1"
  }
}
